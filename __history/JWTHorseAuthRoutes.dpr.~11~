{Protecci{on de las rutas}
program JWTHorseAuthRoutes;

{$APPTYPE CONSOLE}

{$R *.res}

uses Horse, Horse.JWT;

begin
  THorse.Use(HorseJWT('key'));

  THorse.Post('ping',
    procedure(Req: THorseRequest; Res: THorseResponse; Next: TProc)
    begin
      Res.Send('pong');
    end);

  THorse.Listen(9001);
end.
